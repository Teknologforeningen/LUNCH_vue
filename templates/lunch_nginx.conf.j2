server {
  access_log /var/log/nginx/lunch-access.log;
  error_log /var/log/nginx/lunch-error.log warn;

  listen       80;
  server_name  lunch.dvalin.tf.fi;
  location / {
    root   /srv/lunch-frontend;
    index  index.html;
    try_files $uri $uri/ /index.html;
  }
  error_page   500 502 503 504  /50x.html;
  location = /50x.html {
    root   /usr/share/nginx/html;
  }
}
